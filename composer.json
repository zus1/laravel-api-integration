{
    "name": "zus1/laravel-api-integration",
    "description": "Laravel library for integrating 3rd party apis",
    "keywords": ["laravel", "api", "php", "client"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Ožuška",
            "email": "mario.ozuska@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^11.0",
        "illuminate/support": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Zus1\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zus1\\Api\\Providers\\ApiIntegrationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
